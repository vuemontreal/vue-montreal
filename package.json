{
  "name": "vuemontreal",
  "version": "2.0.0",
  "description": "Vue montreal community website",
  "author": "f3ltron",
  "contributors": [
    {
      "name": "Florent Giraud"
    }
  ],
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "chromatic": "chromatic",
    "storybook": "start-storybook -p 6060",
    "build-storybook": "build-storybook -c .storybook"
