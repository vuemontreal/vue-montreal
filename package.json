    "@nuxtjs/pwa": "^3.0.0-0",
    "dotenv": "^8.2.0",
    "highlight.js": "^10.0.2",
    "markdown-it": "^10.0.0",
    "nuxt": "^2.12.0",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-i18n": "^6.6.1",
    "storyblok-nuxt": "^1.1.0",
    "swiper": "^5.3.6",
    "tailwindcss-aspect-ratio": "^3.0.0",
